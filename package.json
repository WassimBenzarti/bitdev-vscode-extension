{
	"name": "bitdev",
	"displayName": "Bitdev",
	"publisher": "wassimbenzarti",
	"description": "Making bit.dev commands easier with this unofficial extension",
	"version": "0.3.4",
	"repository": {
		"type": "git",
		"url": "https://github.com/WassimBenzarti/bitdev-vscode-extension.git"
	},
	"author": {
		"name": "Wassim Benzarti",
		"url": "https://github.com/WassimBenzarti"
	},
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"SCM Providers",
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World",
				"category": "Bit"
			},
			{
				"command": "extension.bitSetup",
				"title": "Setup",
				"category": "Bit"
			},
			{
				"command": "extension.bitLogin",
				"title": "Login",
				"category": "Bit"
			},
			{
				"command": "extension.bitAdd",
				"title": "Add Component",
				"category": "Bit"
			},
			{
				"command": "extension.bitList",
				"title": "List components",
				"category": "Bit"
			},
			{
				"command": "extension.bitImport",
				"title": "Import component",
				"category": "Bit"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js",
		"deploy": "vsce publish --yarn",
		"package": "vsce package --yarn -o bitdev.vsix"
	},
	"devDependencies": {
		"@types/debounce": "^1.2.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/node-fetch": "^2.5.4",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vsce": "^1.71.0",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"debounce": "^1.2.0",
		"node-fetch": "^2.6.0"
	}
}
