{
	"name": "bitdev",
	"displayName": "Bitdev",
	"publisher": "wassimbenzarti",
	"description": "Making bit.dev commands easier with this unofficial extension",
	"version": "0.3.5",
	"repository": {
		"type": "git",
		"url": "https://github.com/WassimBenzarti/bitdev-vscode-extension.git"
	},
	"author": {
		"name": "Wassim Benzarti",
		"url": "https://github.com/WassimBenzarti"
	},
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"SCM Providers",
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"views": {
			"bit-components": [
				{
					"id": "bitComponents",
					"name": "Bit Components"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "bit-components",
					"title": "Bit Explorer",
					"icon": "media/activity-bar-logo.png"
				}
			]
		},
		"commands": [
			{
				"command": "bitdev.bitSetup",
				"title": "Setup",
				"category": "Bit"
			},
			{
				"command": "bitdev.bitLogin",
				"title": "Login",
				"category": "Bit"
			},
			{
				"command": "bitdev.bitAdd",
				"title": "Add Component",
				"category": "Bit"
			},
			{
				"command": "bitdev.bitList",
				"title": "List components",
				"category": "Bit"
			},
			{
				"command": "bitdev.bitRefresh",
				"title": "Refresh components",
				"category": "Bit",
				"icon":"$(refresh~spin)"
			},
			{
				"command": "bitdev.bitImport",
				"title": "Import component",
				"category": "Bit"
			},
			{
				"command": "bitdev.bitTagPublish",
				"title": "Tag and publish component",
				"category": "Bit"
			},
			{
				"command": "bitdev.bitUntrack",
				"title": "Untrack a component"
			}
		],
		"menus": {
			"view/title": [
			  {
				"command": "bitdev.bitRefresh",
				"when": "view == bitComponents",
				"group": "navigation"
			  }
			],
			"view/item/context": [
				{
				  "command": "bitdev.bitUntrack",
				  "when": "view == bitComponents && viewItem == component"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js",
		"deploy": "vsce publish --yarn",
		"package": "vsce package --yarn -o bitdev.vsix"
	},
	"devDependencies": {
		"@types/debounce": "^1.2.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/node-fetch": "^2.5.4",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vsce": "^1.71.0",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"debounce": "^1.2.0",
		"node-fetch": "^2.6.0"
	}
}